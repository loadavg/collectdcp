<?xml version="1.0" encoding="UTF-8"?>

<interface>
  <requires lib="gtk+" version="3.8"/>
  <object class="GtkLabel" id="conntrack">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <property name="label">Plugin conntrack


This plugin collects IP conntrack statistics.

</property>
    <property name="use_markup">True</property>
  </object>
  <object class="GtkGrid" id="conntrack">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <child>
      <object class="GtkLabel" id="label-OldFiles">
	<property name="visible">True</property>
	<property name="can_focus">False</property>
	<property name="label" translatable="yes">OldFiles</property>
      </object>
      <packing>
	<property name="left_attach">0</property>
	<property name="top_attach">0</property>
	<property name="width">1</property>
	<property name="height">1</property>
      </packing>
    </child>
    <child>
      <object class="GtkEntry" id="OldFiles">
	<property name="visible">True</property>
	<property name="can_focus">True</property>
	<property name="tooltip_markup" translatable="yes">
Assume the conntrack_count and conntrack_max files to be found in
/proc/sys/net/ipv4/netfilter instead of /proc/sys/net/netfilter/.
</property>
	<property name="placeholder_text" translatable="yes"></property>
      </object>
      <packing>
	<property name="left_attach">1</property>
	<property name="top_attach">0</property>
	<property name="width">1</property>
	<property name="height">1</property>
      </packing>
    </child>
  </object>
</interface>