<?xml version="1.0" encoding="UTF-8"?>

<interface>
  <requires lib="gtk+" version="3.8"/>
  <object class="GtkLabel" id="nut">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <property name="label">Plugin nut

</property>
    <property name="use_markup">True</property>
  </object>
  <object class="GtkGrid" id="nut">
    <property name="visible">True</property>
    <property name="can_focus">False</property>
    <child>
      <object class="GtkLabel" id="label-UPS">
	<property name="visible">True</property>
	<property name="can_focus">False</property>
	<property name="label" translatable="yes">UPS</property>
      </object>
      <packing>
	<property name="left_attach">0</property>
	<property name="top_attach">0</property>
	<property name="width">1</property>
	<property name="height">1</property>
      </packing>
    </child>
    <child>
      <object class="GtkEntry" id="UPS">
	<property name="visible">True</property>
	<property name="can_focus">True</property>
	<property name="tooltip_markup" translatable="yes">
Add a UPS to collect data from. The format is identical to the one accepted by
upsc(8).
</property>
	<property name="placeholder_text" translatable="yes"></property>
      </object>
      <packing>
	<property name="left_attach">1</property>
	<property name="top_attach">0</property>
	<property name="width">1</property>
	<property name="height">1</property>
      </packing>
    </child>
  </object>
</interface>